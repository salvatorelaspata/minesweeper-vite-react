{
  "name": "vite-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "build:socket": "vite build --base=./ . --outDir=./dist",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "start": "vite build && ssc build -r",
    "version": "ssc -v",
    "build:win": "dir && ssc build",
    "build:ubuntu": "ssc build",
    "build:android": "ssc build --platform=android",
    "build:ios": "ssc build --platform=android"
  },
  "dependencies": {
    "@socketsupply/socket": "^0.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.17.0",
    "valtio": "^1.11.3"
  },
  "devDependencies": {
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "typescript": "^5.3.3",
    "vite": "^6.3.2",
    "vite-plugin-pwa": "^1.0.0",
    "workbox-window": "^7.0.0"
  }
